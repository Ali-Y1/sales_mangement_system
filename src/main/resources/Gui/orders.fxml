<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@../styles/order.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Gui.OrderController">
   <children>
      <HBox prefHeight="21.0" prefWidth="316.0">
         <children>
            <Label prefHeight="31.0" prefWidth="69.0" text="Orders">
               <font>
                  <Font name="Microsoft New Tai Lue" size="21.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <Button mnemonicParsing="false" prefHeight="16.0" prefWidth="94.0" styleClass="createBT" text="Create Orders">
               <HBox.margin>
                  <Insets top="5.0" />
               </HBox.margin>
            </Button>
            <Region prefHeight="32.0" prefWidth="135.0" HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" prefHeight="16.0" prefWidth="82.0" text="Export" textFill="#5d3c75">
               <graphic>
                  <FontAwesomeIconView fill="#5d3c75" glyphName="UPLOAD" />
               </graphic>
               <HBox.margin>
                  <Insets right="7.0" top="7.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <TabPane styleClass="pan" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
        <tabs>
          <Tab text="All Orders">
            <content>
              <AnchorPane>
                     <children>
                        <TableView fx:id="tableOrders" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                          <columns>
                            <TableColumn fx:id="IdCol" prefWidth="75.0" text="ID">
                            </TableColumn>
                            <TableColumn prefWidth="75.0" text="Customer">
                            </TableColumn>
                              <TableColumn fx:id="StatusCol" prefWidth="75.0" text="Status">
                              </TableColumn>
                              <TableColumn fx:id="PriceCol" prefWidth="75.0" text="Price">
                              </TableColumn>
                          </columns>
                        </TableView>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Pendding">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
        </tabs>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </TabPane>
   </children>
</VBox>
